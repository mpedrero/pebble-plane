{
  "name": "pebble-plane",
  "author": "Manuel Pedrero",
  "version": "1.5.0",
  "keywords": [
    "simple",
    "digital",
    "time",
    "health"
  ],
  "private": false,
  "dependencies": {
    "pebble-clay": "^1.0.2"
  },
  "pebble": {
    "displayName": "Plane",
    "uuid": "cb64d11d-c138-4b35-8771-6792b68d5eae",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": [
      "theme",
      "language",
      "bt_alarm",
      "steps",
      "leading_zero"
    ],
    "resources": {
      "media": [
        {
          "type": "font",
          "name": "FONT_HEL_LIG_70",
          "characters": "[0-9]",
          "file": "fonts/Helvetica.ttf",
          "compatibility": "2.7"
        },
        {
          "type": "font",
          "name": "FONT_HEL_LIG_21",
          "characters": "[0-9:A-Za-z,% ]",
          "file": "fonts/Helvetica.ttf"
        }
      ]
    },
    "capabilities": [
      "health",
      "configurable"
    ]
  }
}
